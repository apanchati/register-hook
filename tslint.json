{
  "extends": [
    "tslint:latest"
  ],
  "rules": {
    "semicolon": false,
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "prefer-const": false,
    "no-submodule-imports": false,
    "no-implicit-dependencies": false,
    "trailing-comma": false,
    "object-literal-sort-keys": false,
    "max-line-length": [
      true,
      200
    ],
    "prefer-conditional-expression": false,
    "ordered-imports": false,
    "no-consecutive-blank-lines": false,
    "prefer-for-of":false
  }
}
